<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <environments>Default</environments>
    <formulas>
        <name>apptierclientname</name>
        <dataType>String</dataType>
        <expression>{!$Record.caTag_Product__c} &amp; &quot; - &quot; &amp; {!$Record.caTag_Environment__c}</expression>
    </formulas>
    <interviewLabel>AWS S3 Bucket App Attachment {!$Flow.CurrentDateTime}</interviewLabel>
    <label>AWS S3 Bucket App Attachment</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>set_app_tier_client_name</name>
        <label>set app tier client name</label>
        <locationX>176</locationX>
        <locationY>323</locationY>
        <inputAssignments>
            <field>CA10__applicationTierClientName__c</field>
            <value>
                <elementReference>apptierclientname</elementReference>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>set_app_tier_client_name</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>caTag_Environment__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <filters>
            <field>caTag_Product__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue></stringValue>
            </value>
        </filters>
        <object>CA10__CaAwsBucket__c</object>
        <recordTriggerType>CreateAndUpdate</recordTriggerType>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
